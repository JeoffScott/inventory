                <settings>
                    <label translate="true">Qty</label>
                    <scopeLabel>[GLOBAL]</scopeLabel>
                    <validation>
                        <rule name="less-than-equals-to" xsi:type="number">99999999</rule>
                        <rule name="validate-number" xsi:type="boolean">true</rule>
                    </validation>
                    <dataScope>inventory</dataScope>
